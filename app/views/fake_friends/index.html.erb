<% @fake_friends.each do |fake_friend| %>
  <h1>
    <%= link_to fake_friend.name, fake_friend_path(fake_friend) %>
  </h1>
  <p><%= fake_friend.gender %></p>
  <p><%= fake_friend.address %></p>
  <p><%= fake_friend.age %></p>
  <p><%= fake_friend.main_description %></p>
  <p><%= fake_friend.rating %></p>

<% end %>

<p><%= link_to "add a new fake_friend", new_fake_friend_path %></p>

